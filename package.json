  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "del-cli": "^1.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.13.3",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "simplelightbox": "^1.12.1"
